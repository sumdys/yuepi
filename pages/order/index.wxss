.container{
  width: 100%;
}
.wrap{
  width: 90%;
  margin: 30px auto;
}
.header{
  display: -webkit-flex;
  display: -ms-flex;
  display: flex;
  width: 100%;
  height: 40px;
  font-size: 18px;
  line-height: 40px;
  margin-bottom: 10px;
  border-top: 1px solid #dadada;
  border-bottom: 1px solid #dadada;
}
.kind{

  flex:1;
  box-sizing: border-box;
  color: #000;
  font-weight: 500;

}
.state{
  color: red;
  margin-right: 10px;
}
.info{
  width: 100%;
  font-size: 14px;
  margin-bottom: 20px;
}
.label{
  color: #999;
}
.content{
  color: #000;
}
.start{
  width: 90%;
  margin: 150px auto;
  text-align: center;
}
.warn{
  color: #00beaf;
}
